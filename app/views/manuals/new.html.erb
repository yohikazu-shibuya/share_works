<%= render "devise/shared/header" %>

<%= form_with model: @manual, local: true do |f|%>

<%= f.text_area :genre, placeholder:"例) カテゴリ" %><br>

<%= f.text_area :subject, placeholder:"例) 件名" %><br>

<%= f.text_area :notice, placeholder:"例) 質問" %><br>
<%= f.submit "保存する", class: :form__btn %>

<% end %>